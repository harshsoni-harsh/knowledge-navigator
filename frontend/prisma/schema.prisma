generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model FileMetadata {
  id          String    @id @default(uuid())
  name        String
  size        String
  created_at  String
  year        String
  modified_at String
  author      String
}
